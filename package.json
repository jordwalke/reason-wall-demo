
{
  "name": "reason-wall-demo",
  "version": "0.0.1",
  "dependencies": {
    "@opam/wall": "let-def/wall#8b47b5ce898f6b35d6cbf92aa12baadd52f05350",
    "@esy-ocaml/esy-installer": "^0.0.0",
    "@esy-ocaml/substs": "^0.0.1",
    "@opam/grenier": "*",
    "@opam/ocamlfind": "",
    "@opam/stb_image": "*",
    "@opam/stb_truetype": "*",
    "@opam/tgls": "*",
    "@opam/tsdl": "*",
    "@esy-ocaml/reason": "^3.0.0",
    "refmterr": "*"
  },
  "esy": {
    "build": [
      [ "refmterr", "jbuilder", "build",
        "bin/gallery/ReasonWallGallery.exe",
        "bin/ui/ReasonWallUi.exe",
        "@install"
      ]
    ],
    "install": [
      [ "esy-installer" ]
    ],
    "buildsInSource": "_build",
    "release": {
      "releasedBinaries": [
        "ReasonWallDemo.exe"
      ],
      "NOTES": "Below, we just strip out binary builds for things that aren't needed at runtime.",
      "NOTES": "Make your release as lean as you like!",
      "deleteFromBinaryRelease": [
        "*menhir-*",
        "*lambda_term-*",
        "*ocaml_migrate_parsetree-*",
        "*zed-*",
        "*wall-*",
        "*ocamlfind-*",
        "*refmterr-*",
        "*lambda_term-*",
        "*re-*",
        "*utop-*",
        "*camlp4-*",
        "*esy_ocaml__slash__esy_installer-*",
        "*topkg-*",
        "*ocaml-4*",
        "*reason-*",
        "*camlp4-*",
        "*camomile-*",
        "*esy_ocaml__slash__substs-*",
        "*conf_which-*",
        "*conf_m4-*",
        "*cppo-*",
        "*jbuilder-*",
        "*astring-*",
        "*base_bytes-*",
        "*ppx___tools___versioned-*",
        "*grenier-*"
      ]
    }
  },
  "peerDependencies": {
    "ocaml": "~4.6.0"
  },
  "devDependencies": {
    "@esy-ocaml/merlin": "3.0.5000",
    "ocaml": "~4.6.0"
  },
  "resolutions": {
    "**/@opam/result": "1.2.0"
  },
  "scripts": {
    "test": "./test.sh"
  }
}
